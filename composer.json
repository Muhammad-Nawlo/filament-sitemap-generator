{
    "name": "muhammad-nawlo/filament-sitemap-generator",
    "description": "Filament plugin to generate XML sitemaps with splitting, news, images, and search-engine ping",
    "keywords": [
        "Muhammad-Nawlo",
        "laravel",
        "filamentphp",
        "filament",
        "filament-plugin",
        "filament-sitemap-generator"
    ],
    "homepage": "https://github.com/muhammad-nawlo/filament-sitemap-generator",
    "support": {
        "issues": "https://github.com/muhammad-nawlo/filament-sitemap-generator/issues",
        "source": "https://github.com/muhammad-nawlo/filament-sitemap-generator"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad-Nawlo",
            "email": "nawlomuhammadit@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.2 || ^4.0 || ^5.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "spatie/laravel-sitemap": "^7.4"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.7|^4.0",
        "pestphp/pest-plugin-arch": "^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^3.0|^4.0",
        "pestphp/pest-plugin-livewire": "^3.0|^4.0",
        "rector/rector": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "MuhammadNawlo\\FilamentSitemapGenerator\\": "src/",
            "MuhammadNawlo\\FilamentSitemapGenerator\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MuhammadNawlo\\FilamentSitemapGenerator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "phpstan analyse",
        "lint": "pint",
        "refactor": "rector",
        "test": "pest",
        "test:lint": "pint --test",
        "test:refactor": "rector --dry-run"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuhammadNawlo\\FilamentSitemapGenerator\\FilamentSitemapGeneratorServiceProvider"
            ],
            "aliases": {
                "FilamentSitemapGenerator": "MuhammadNawlo\\FilamentSitemapGenerator\\Facades\\FilamentSitemapGenerator"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.0.0"
}
